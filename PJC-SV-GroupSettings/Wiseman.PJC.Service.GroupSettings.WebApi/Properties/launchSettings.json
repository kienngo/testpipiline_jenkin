{
  // VisualStudioからの起動時にのみ使用される起動プロファイルです。
  // KestrelのIP/Portを指定することもできますがMVRSではKestrel構成ファイルで指定するので
  // VisualStudioからの起動時にENVIRONMENTをDevelopに設定し起動するためと
  // 起動時Swaggerの有効化およびブラウザ自動起動等の設定に使用します

  "profiles": {
    "WebApi": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "https://localhost:10001;http://localhost:10000"
    }
  }
}